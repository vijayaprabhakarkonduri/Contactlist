<div class="section-head">
	<h3><img src="/css/images/smallcalendar.png" />Timelines</h3>
</div>
<div class="timelines">
	<table class="table">
		<thead class="timelines-table-head">
			<th>Deliverables</th>
			<th>Type Of Document</th>
			<th>Start Date & Time</th>
			<th>Submission Date & Time</th>
			<th>Duration</th>
		</thead>
		<tbody class="timelines-table-body">
			<tr ng-repeat="x in repeat track by $index" ng-mouseenter="cancel=true" ng-mouseleave="cancel=false" class="timelines_row" ng-init="track[$index].show=true">
				<td class="artifact-suggest">
					<input type="text" placeholder="Deliverables" ng-model="x.deliverable" ng-focus="removeClass($event);" ng-click="datePicker()"  ng-blur="checkArtifact(x.deliverable)"/>
					<!-- <div ng-if="x.deliverable && track[$index].show" class="artifact-suggestions-block" ng-keyup="select($event, $index)track[$index].show=true;">
						<div ng-repeat="artifact in artifacts  | filter : x.deliverable | limitTo: 3" class="artifact-suggestions" ng-click="x.deliverable = artifact.artifact; track[$index].show=false;" ng-if="x.deliverable != artifact.artifact">{{artifact.artifact}}</div>
					</div> -->
				</td>
				<td class="">
					<select ng-model="x.type">
						<option value="" disabled selected>Select type of document</option>
						<option>Document</option>
						<option>Sheet</option>
						<option>Prototypes</option>
						<option>Presentation</option>
					</select>
				</td>
				<td class="start-date">
					<div class="date-icon">
						<input type="text" class="from start_date_input" ng-model="x.start_date" placeholder="Start time" readonly />
					</div>
					<select class="start_time" ng-model="x.start_time" ng-selected="nextTime">
						<option value="" disabled selected>hh.mm</option>
						<option>09.00</option>
						<option>09.15</option>
						<option>09.30</option>
						<option>09.45</option>
						<option>10.00</option>
						<option>10.15</option>
						<option>10.30</option>
						<option>10.45</option>
						<option>11.00</option>
						<option>11.15</option>
						<option>11.30</option>
						<option>11.45</option>
						<option>12.00</option>
						<option>12.15</option>
						<option>12.30</option>
						<option>12.45</option>
						<option>12.00</option>
						<option>13.15</option>
						<option>13.30</option>
						<option>13.45</option>
						<option>14.00</option>
						<option>14.15</option>
						<option>14.30</option>
						<option>14.45</option>
						<option>15.00</option>
						<option>15.15</option>
						<option>15.30</option>
						<option>15.45</option>
						<option>16.00</option>
						<option>16.15</option>
						<option>16.30</option>
						<option>16.45</option>
						<option>17.00</option>
						<option>17.15</option>
						<option>17.30</option>
						<option>17.45</option>
						<option>18.00</option>
					</select>
				</td>
				<td class="start-date">
					<div class="date-icon">
						<input type="text" class="to submission_date" ng-model="x.submission_date" placeholder="Submission time" readonly />
					</div>
					<select class="submission_time" ng-model="x.submission_time">
						<option value="" disabled selected>hh.mm</option>
						<option>09.00</option>
						<option>09.15</option>
						<option>09.30</option>
						<option>09.45</option>
						<option>10.00</option>
						<option>10.15</option>
						<option>10.30</option>
						<option>10.45</option>
						<option>11.00</option>
						<option>11.15</option>
						<option>11.30</option>
						<option>11.45</option>
						<option>12.00</option>
						<option>12.15</option>
						<option>12.30</option>
						<option>12.45</option>
						<option>12.00</option>
						<option>13.15</option>
						<option>13.30</option>
						<option>13.45</option>
						<option>14.00</option>
						<option>14.15</option>
						<option>14.30</option>
						<option>14.45</option>
						<option>15.00</option>
						<option>15.15</option>
						<option>15.30</option>
						<option>15.45</option>
						<option>16.00</option>
						<option>16.15</option>
						<option>16.30</option>
						<option>16.45</option>
						<option>17.00</option>
						<option>17.15</option>
						<option>17.30</option>
						<option>17.45</option>
						<option>18.00</option>
					</select>
				</td>
				
				<td class="duration">
					<input ng-value="calDur(x.start_date,x.start_time,x.submission_date,x.submission_time, $index)" placeholder="hh.mm" readonly ng-model="x.duration" ng-change="cal_total_duration()" />
					<div>
						<img src="/css/images/delete.png" ng-show="cancel && repeat.length > 3" ng-click="remove($index)" />
					</div>
				</td>
			</tr>
			<tr>
				<td class="total-duration"colspan="4">
					<div class="col-md-12 addstudent go-to-library" ng-click="add()">
						<img src="/css/images/plus.png" class="" />
					</div>
				</td>
				<td class="duration">
					<p>Total Duration</p>
					<span ng-value="cal_total_duration()">{{total_duration}}</span>
				</td>
			</tr>
			<tr class="timelines-button">
				<td colspan="4">

				</td>
				<td>
					<button ng-click="setTimelines()">
						Set Timelines
					</button>
				</td>
			</tr>
		</tbody>
	</table>
</div>