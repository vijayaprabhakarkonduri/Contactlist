<div class="col-md-12 archieved-projects">
	<div class="redflag-head">
		<h3><img src="/css/images/folder.png" />Archived Projects</h3>
	</div>
	<div class="col-md-3 archeived-tiles" ng-repeat="archieved in archieved_projects">
		<div class="redflag-mentee">
			<div class="student-task-details">
				<div class="task-name">
					<h4 class="each-task-name">{{archieved.name}}</h4>
					<div></div>	
					<p class="mentee-last-used" >{{archieved.last_update}}</p>
				</div>	
			</div>
			<div class="task-status task-deadline">
				<div class="artifact-name" ng-repeat="skill_object in archieved.skills | orderBy : 'score' | limitTo :3">
					<span>{{skill_object.skill}}</span>
				</div>
			</div>
			<div class="col-md-offset-8 pull-right">
				<p>Mentor Rating</p>
				<div class="peer-rating">
					<h2><strong>{{archieved.rating}}</strong></h2>
				</div>
			</div>
			<div class="clearfix"></div>
			<div class="review-work">
				<img src="/css/images/mentor.png">
				<span>{{archieved.mentor}}</span>
			</div>
		</div>
	</div>
</div>