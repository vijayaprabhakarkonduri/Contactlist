<!doctype html>
<html>
<head>
	<title><%= title %></title>
	<%- include('partials/css_includes') %>
</head>
<body>
	<div class="login-wrapper">
		<div class="login">
			<img src="../css/images/logo_uxd_login.png" />
			<form class="center-screen login-dialog" action="/login" method="post">
				<div class="form-group">
					<input type="text" name="username" class="form-control" id="exampleInputEmail1" placeholder="Email">
				</div>
				<div class="form-group">
					<input type="password" name="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
				</div>
				<button type="submit" value="Submit" class="btn btn-default">Submit</button> 
				<a href="">Forgot Password?</a>
			</form>
		</div>	
	</div>
</body>
</html>
